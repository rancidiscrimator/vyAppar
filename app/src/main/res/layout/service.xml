<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        card_view:cardElevation="0dp"
        card_view:layout_constraintBottom_toBottomOf="parent"
        card_view:layout_constraintEnd_toEndOf="parent"
        card_view:layout_constraintStart_toStartOf="parent"
        card_view:layout_constraintTop_toTopOf="parent"
        card_view:layout_constraintVertical_bias="0.0">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/iw1"
                android:layout_width="85dp"
                android:layout_height="85dp"
                android:layout_marginLeft="15dp"
                android:src="@drawable/foodplaceholder"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/twDesc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="14dp"
                android:layout_marginTop="8dp"
                android:ems="10"
                android:fontFamily="@font/oxygen"
                android:inputType="textPersonName"
                android:text="Tasty Homemade Malayam"

                android:textColor="@color/greybar"
                android:textSize="14sp"
                card_view:layout_constraintBottom_toBottomOf="parent"
                card_view:layout_constraintEnd_toEndOf="@+id/tv1"
                card_view:layout_constraintHorizontal_bias="0.0"
                card_view:layout_constraintStart_toEndOf="@+id/iw1"
                card_view:layout_constraintTop_toBottomOf="@+id/tv1"
                card_view:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/tv1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="14dp"
                android:ems="10"
                android:fontFamily="@font/oxygen"
                android:inputType="textPersonName"
                android:text="Malayalam Food"
                android:textColor="#000000"
                android:textSize="16sp"
                android:textStyle="bold"
                card_view:layout_constraintStart_toEndOf="@+id/iw1"
                card_view:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tvMisc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="56dp"
                android:fontFamily="@font/oxygen"
                android:text="20$"
                android:textColor="@color/black"
                android:textSize="16sp"
                android:textStyle="bold"
                card_view:layout_constraintBottom_toBottomOf="@+id/tv1"
                card_view:layout_constraintStart_toEndOf="@+id/tv1"
                card_view:layout_constraintTop_toTopOf="parent"
                card_view:layout_constraintVertical_bias="0.0" />
            <ImageView
                android:layout_width="85dp"
                android:layout_height="85dp"
                android:src="@drawable/outline"
                card_view:layout_constraintBottom_toBottomOf="@id/iw1"
                card_view:layout_constraintStart_toStartOf="@+id/iw1"
                card_view:layout_constraintTop_toTopOf="@+id/iw1"
                card_view:layout_constraintEnd_toEndOf="@id/iw1"/>
            <ImageView
                android:layout_width="85dp"
                android:layout_height="85dp"
                android:src="@drawable/outline1"
                card_view:layout_constraintBottom_toBottomOf="@id/iw1"
                card_view:layout_constraintStart_toStartOf="@+id/iw1"
                card_view:layout_constraintTop_toTopOf="@+id/iw1"
                card_view:layout_constraintEnd_toEndOf="@id/iw1"/>

        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>